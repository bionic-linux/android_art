arg_expected="$1"
arg_output="$2"

# Strip 'JNI_OnLoad called' in case its printed multiple times, and its irrelevant to our test.
sed -i "/JNI_OnLoad called/d" "$arg_output"

# If printing "No OAT class" (see Main.java),
# the test is vacuously true, so return 'Success' here.
if diff expected.no-compiled-code.txt "$arg_output" > /dev/null; then
  exit 0
fi

# Compare against whatever happens to be the current expected.txt
./default-check "$@"
